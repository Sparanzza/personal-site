<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="web">
    <inherits name="com.google.gwt.user.User"/>

    <inherits name="com.sparanzza.website.Shared"/>
    <inherits name="com.google.gwt.user.theme.clean.Clean"/>
    <inherits name='com.google.gwt.json.JSON'/>
    <inherits name="com.intendia.rxgwt2.RxUser"/>
    <inherits name="org.jboss.gwt.elemento.Core"/>
    <inherits name="com.intendia.reactivity.Reactivity"/>
    <inherits name="dagger.Dagger"/>

    <set-property name="compiler.useSourceMaps" value="true"/>
    <set-property name="compiler.useSymbolMaps" value="true"/>

    <set-configuration-property name="CssResource.enableGss" value="true"/>

    <public path="public"/>
    <source path="client"/>

    <entry-point class="com.sparanzza.website.client.ApplicationEntryPoint"/>
    <!-- Only support recent browsers -->
    <set-property name="user.agent" value="ie10,gecko1_8,safari"/>
</module>
