/*FOOTER*/
footer{ clear: both;}
.footer-sign {
    text-align: center;
    font-size: 20px;
    font-weight: 300;
}

footer > * > * { color : #5b708b; }

.footer-social-link { display: flex; justify-content: center; margin: 40px 0px;}
.footer-social-link > a > i {
    border-radius: 100%;
    border: 2px solid rgba(0,0,0,0.3);
    margin: 14px;
    color: #5b708b;
    font-size: 18px;
    transition: .3s all;
    box-sizing: content-box;
    width: 50px;
    height: 50px;
    text-align: center;
    line-height: 49px;
}

.footer-social-link > a > i:hover{
    background: lightcyan;
    transition: .3s all;
    transform: scale(1.05);
    cursor: pointer;
}

footer .made-with{
    text-align: center;
    font-size: 13px;
    line-height: 20px;
}

.footer-link > a {
    color: #5b708b;
    display: block;
}

/* SKILLS HOME */
.container-skills-home{
    top: -150px;
    position: relative;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin: 20px 4px;
}

.container-skills-home > div{
    position: relative;
    flex: 1 1 265px;
    background-color: floralwhite;
    margin: 4px;
    border-radius: 10px;
    border: 1px solid lavender;
    -webkit-box-shadow: 0 10px 38px 6px rgba(148,230,255,0.50);
    -moz-box-shadow: 0 10px 38px 6px rgba(148,230,255,0.50);
    box-shadow: 0 10px 9px -6px rgba(54,101,115,0.22);
    padding: 50px 0px;
}

.container-skills-home > div > img {
    width: 60px;
    margin: 20px;
}

.container-skills-home > div > div > p { padding: 12px; line-height: 24px; }
p > strong{ font-weight: 700; color: #9300fff5; }
.container-skills-home > div > ul { list-style: none; }
