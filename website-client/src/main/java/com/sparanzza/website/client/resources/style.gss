    width: 100%;
    background-color: #00ffdc;
    margin-top: 50px;
    -webkit-box-shadow: 0px 10px 38px 6px rgba(148,230,255,0.50);
    -moz-box-shadow: 0px 10px 38px 6px rgba(148,230,255,0.50);
    box-shadow: 0px 10px 38px 6px rgba(148,230,255,0.50);
}

.purple-shadow-section{
    padding: 30px;
    width: 100%;
    background-color: #e5c3ff;
    margin-top: 50px;
    -webkit-box-shadow: 0px 10px 38px 6px rgba(214,148,255,0.35);
