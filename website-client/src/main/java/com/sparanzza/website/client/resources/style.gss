@external "gwt-*";


* { margin: 0px; padding: 0px; }

body{
   font-family: 'Montserrat', sans-serif;
   font-size: 15px;
   font-weight: 400;
   line-height: 29px;
   letter-spacing: -.02em;
   color : #5b708b;
   background: floralwhite;
}

h1{ font-size: 48px; font-weight: 700; line-height: 48px;}
h2{ font-size: 28px; font-weight: 700; line-height: 28px;}
h3{ font-size: 22px; font-weight: 600; line-height: 22px;}
h3{ font-size: 15px; font-weight: 500; line-height: 15px;}
h5{ font-size: 18px; font-weight: 300; }
p{ text-align: justify; text-justify: inter-word; }


.mv20{ margin: 20px 0px; }
.mv50{ margin: 50px 0px; }
.mv100{ margin: 100px 0px; }

/*TOP NAVBAR*/
/* Add a black background color to the top navigation */
.topnav { overflow: hidden; }

/* Style the links inside the navigation bar */
.topnav a {
    position: relative;
    float: left;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
    font-size: 14px;
    font-weight: 600;
    cursor: pointer;
}


/* Change the color of links on hover */
.topnav a:hover {
    text-decoration: none;
    position: relative;
}

.topnav a:before {
    content: "";
    position: absolute;
    width: 100%;
    height: 2px;
    bottom: 10px;
    left: 0;
    background-color: #ff23b9;
    visibility: hidden;
    -webkit-transform: scaleX(0);
    transform: scaleX(0);
    -webkit-transition: all .5s cubic-bezier(1,.25,0,.75) 0s;
    transition: all .5s cubic-bezier(1,.25,0,.75) 0s;
}

.topnav a:hover:before {
    visibility: visible;
    -webkit-transform: scaleX(1);
    transform: scaleX(.8);
}

.topnav a.active { color: #ff23b9; }

/*ALERT DIALOG*/
.alertdialog {
    margin-bottom: 10px;
    padding: 10px;
    background-color: #f44336;
    font-weight: 500;
    font-size: 12px;
    line-height: 18px;
    letter-spacing: .3px;
    transition: 0.3s;
    color: white;
 }

.alertdialog a {
    color: white;
    font-weight: 700;
    margin: 5px 0px;
}
 .alertdialog .alertdialog-closebtn{
    margin-right: 15px;
    font-weight: bold;
    font-size: 16px;
    line-height: 16px;
 }

/*MAGICAL UNDERLINE FOR PARAGRAPH*/
.underline-magical {
    background-image: linear-gradient(120deg, #84fab0 0%, #8fd3f4 100%);
    background-repeat: no-repeat;
    background-size: 100% 0.2em;
    background-position: 0 88%;
    transition: background-size 0.25s ease-in;
}
underline-magical:hover {
    background-size: 100% 88%;
}


.cyan-shadow-section{
